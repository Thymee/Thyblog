  "scripts": {
    "start":"'babel-node' .bin/www",
    "pm2": "pm2 start processes.json",
    "dev-a": "babel-node admin/build/dev-server.js",
    "build-a": "node admin/build/build.js",
    "dev-s": "babel-node server/bin/run",
    "pro-s":"NODE_ENV=production && npm babel-node server/bin/run",
    "dev-c": "babel-node client/build/dev-server.js",
    "build-c": "babel-node client/build/build.js" ,
    "fdc": "forever start --minUptime 1000 --spinSleepTime 1000 client/build/dev-server.js",
    "sdc": "forever stop client/build/dev-server.js",
    "fda": "forever start --minUptime 1000 --spinSleepTime 1000 admin/build/build.js",
    "sda": "forever stop admin/build/build.js",
    "fds": "forever start --minUptime 1000 --spinSleepTime 1000 -c 'babel-node' server/bin/run",
    "sds": "forever stop server/bin/run",
    "dev": "npm run fds && npm run fdc && npm run fda",
    "stop": "npm run sda && npm run sdc && npm run sds"
  },
